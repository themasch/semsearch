{
    "name": "masch/semsearch",
    "description": "sematic search experiment",
    "type": "project",
    "version": "0.0.1",
    "require": {
        "nikic/php-parser": "^4.6",
        "symfony/console": "^5.1",
        "psr/log": "^1.1",
        "symfony/var-dumper": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/yaml": "^5.1",
        "symfony/config": "^5.1",
        "symfony/finder": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "phpstan/phpstan": "^0.12.33",
        "phpstan/phpstan-symfony": "^0.12.7",
        "phpstan/extension-installer": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "masch\\SemSearch\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Schmale",
            "email": "masch@masch.it"
        }
